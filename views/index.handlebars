{{#each articles}}
  <div class="row">
    <div class="col s12 m12">
      <div class="card">
        <div class="card-content">
          <span class="card-title">{{this.title}}</span>
          <p>{{this.excerpt}}</p>
          <img src="{{this.image}}">
        </div>
        <div class="card-action">
          <a class="waves-effect waves-light btn green" href="{{this.link}}">Read</a>
          <a class="waves-effect waves-light btn blue" data-id="{{this.id}}">Notes</a>
          <a class="waves-effect waves-light btn red" data-id="{{this.id}}">Delete Article</a>
        </div>
      </div>
    </div>
  </div>
{{/each}}